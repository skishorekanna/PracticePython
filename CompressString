def compressstring(s):
    if (len(s)<1):
        return s
    result = ""
    cur = s[0]
    counter = 1
    for each in s[1:]:
        if each == cur:
            counter+=1
        else:
            if counter == 1:
                result += cur
            else:
                result += cur + str(counter)
            counter = 1
            cur = each
    if counter == 1:
        result += cur
    else:
        result +=cur + str(counter)
    print resultv
